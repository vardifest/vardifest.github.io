addEventHandler(window,'load',
  function() {
    var anchors = document.getElementsByTagName("a");
    for (var i = anchors.length - 1;i >= 0; i--) {
      var anchor = anchors[i];
      var id = anchor.id;
      if (id && id.substring(0,6) == "_paper") {
        anchor.href = "_paper" + id.substring(6) + ".pdf";
      }
    }
  }
);

